{
  "name": "boxvault-frontend",
  "version": "0.28.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.3",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "i18next": "^25.5.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "loglevel": "^1.9.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.5",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.5.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^7.8.1",
    "sass": "1.77.6",
    "validator": "^13.15.22",
    "vite": "^7.1.11",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "vite",
    "cinstall": "npm ci",
    "cinstall:nodev": "npm ci --omit=dev",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "watch-build": "vite build --emptyOutDir --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "security:audit": "npm audit --audit-level moderate",
    "security:vulnerabilities": "npm audit && better-npm-audit audit",
    "security:licenses": "license-checker-rseidelsohn --onlyAllow 'MIT;Apache-2.0;BSD;BSD-2-Clause;BSD-3-Clause;ISC;GPL-3.0;LGPL-2.1;LGPL-3.0;MPL-2.0;CC0-1.0;CC-BY-3.0;CC-BY-4.0;Unlicense;Python-2.0;Unicode-DFS-2016;Apache 2.0;BlueOak-1.0.0' --excludePrivatePackages",
    "security:supply-chain": "audit-ci --moderate",
    "security:all": "npm run security:audit && npm run security:vulnerabilities && npm run security:licenses && npm run security:supply-chain"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rollup/wasm-node": "^4.24.0",
    "audit-ci": "^7.1.0",
    "better-npm-audit": "^3.11.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "license-checker-rseidelsohn": "^4.4.2",
    "prettier": "^3.7.4",
    "rollup": "npm:@rollup/wasm-node@^4.24.0"
  }
}
